  "scripts": {
    "lint": "yarn standard",
    "test": "mocha test",
    "full-test": "yarn lint && yarn test",
    "clean-docs": "rm -rf ./docs && rm ./API.md",
    "jsdoc": "yarn clean-docs && jsdoc -c .jsdoc.json -R README.md",
